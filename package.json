{
  "name": "venice-staking-bot",
  "version": "1.0.1",
  "description": "Automatic VVV restaking bot",
  "keywords": ["bot", "base", "vvv", "venice", "restake"],
  "author": "Venticello",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
